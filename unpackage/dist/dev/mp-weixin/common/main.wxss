














/*每个页面公共css */
.page{
	position: absolute;
	width: 100%;
	height: 100%;
	background-color: #f7f7f7;
}
.page-block{
	background-color: #ffffff;
}
.avatar {
	width: 134rpx;
	height: 134rpx;
	margin: 0 40rpx 0 30rpx;
	-webkit-border-radius: 50%;
	        border-radius: 50%;
	border: 2px solid #FFFFFF;
	-webkit-box-shadow: 0 0 8px rgba(0, 0, 0, .15);
	        box-shadow: 0 0 8px rgba(0, 0, 0, .15);
}


